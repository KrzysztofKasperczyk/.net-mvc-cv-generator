@model CvGenerator.Models.LanguageItem

<div class="language-item form-row mb-3">
  <div class="form-group col-md-6">
    <label asp-for="Name"></label>
    <input asp-for="Name" id="EntryLanguage_Name" class="form-control" />
  </div>
  <div class="form-group col-md-4">
    <label asp-for="Level"></label>
    <select asp-for="Level" class="form-control">
      <option>A1</option>
      <option>A2</option>
      <option>B1</option>
      <option>B2</option>
      <option>C1</option>
      <option>C2</option>
      <option>Native</option>
    </select>
  </div>
  <div class="form-group col-md-2 d-flex align-items-end">
  </div>
</div>
